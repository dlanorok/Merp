{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Modules": {
    "Accountancy": {
      "ConnectionStrings": {
        "Merp-Accountancy-EventStore": "mongodb://localhost:27017/Merp-Accountancy-EventStore",
        "Merp-Accountancy-ReadModel": "Server=.\\SQLExpress;Database=Merp-Accountancy-ReadModel;Trusted_Connection=True;MultipleActiveResultSets=true",
        "Merp-Accountancy-Drafts": "Server=.\\SQLExpress;Database=Merp-Accountancy-ReadModel;Trusted_Connection=True;MultipleActiveResultSets=true"
      },
      "Rebus": {
        "QueueName": "merp.accountancy",
        "Subscriptions": {
          "ConnectionString": "server=.\\SQLEXPRESS; initial catalog=Merp-Accountancy-Rebus;Integrated Security=True",
          "TableName": "rebus_subscriptions"
        },
        "Sagas": {
          "ConnectionString": "server=.\\SQLEXPRESS; initial catalog=Merp-Accountancy-Rebus;Integrated Security=True",
          "MessagesTableName": "rebus_sagasmessages",
          "IndexesTableName": "rebus_sagasindexes"
        },
        "Transport": {
          "ConnectionString": "server=.\\SQLEXPRESS; initial catalog=Merp-Accountancy-Rebus;Integrated Security=True"
        }
      }
    },
    "Registry": {
      "ConnectionStrings": {
        "Merp-Registry-EventStore": "mongodb://localhost:27017/Merp-Registry-EventStore",
        "Merp-Registry-ReadModel": "Server=.\\SQLExpress;Database=Merp-Registry-ReadModel;Trusted_Connection=True;MultipleActiveResultSets=true"
      },
      "Rebus": {
        "QueueName": "merp.registry",
        "Subscriptions": {
          "ConnectionString": "server=.\\SQLEXPRESS; initial catalog=Merp-Registry-Rebus;Integrated Security=True",
          "TableName": "rebus_subscriptions"
        },
        "Sagas": {
          "ConnectionString": "server=.\\SQLEXPRESS; initial catalog=Merp-Registry-Rebus;Integrated Security=True",
          "MessagesTableName": "rebus_sagasmessages",
          "IndexesTableName": "rebus_sagasindexes"
        },
        "Transport": {
          "ConnectionString": "server=.\\SQLEXPRESS; initial catalog=Merp-Registry-Rebus;Integrated Security=True"
        }
      }
    },
    "TimeTracking": {
      "ConnectionStrings": {
        "Merp-TimeTracking-EventStore": "mongodb://localhost:27017/Merp-TimeTracking-EventStore",
        "Merp-TimeTracking-ReadModel": "Server=.\\SQLExpress;Database=Merp-TimeTracking-ReadModel;Trusted_Connection=True;MultipleActiveResultSets=true"
      },
      "Rebus": {
        "QueueName": "merp.timetracking",
        "Subscriptions": {
          "ConnectionString": "server=.\\SQLEXPRESS; initial catalog=Merp-TimeTracking-Rebus;Integrated Security=True",
          "TableName": "rebus_subscriptions"
        },
        "Sagas": {
          "ConnectionString": "server=.\\SQLEXPRESS; initial catalog=Merp-TimeTracking-Rebus;Integrated Security=True",
          "MessagesTableName": "rebus_sagasmessages",
          "IndexesTableName": "rebus_sagasindexes"
        },
        "Transport": {
          "ConnectionString": "server=.\\SQLEXPRESS; initial catalog=Merp-TimeTracking-Rebus;Integrated Security=True"
        }
      }
    }
  }
}
